{
  "name": "emergent-physics-lab",
  "version": "1.0.0",
  "description": "Interactive physics experiments showing force emergence from lattice field medium",
  "private": true,
  "scripts": {
  "dev": "npm run sync:test-configs && next dev",
  "build": "npm run sync:test-configs && next build",
  "prebuild": "npm run sync:test-configs",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
